import java.util.Iterator;
import java.util.LinkedList;
import java.util.NoSuchElementException;

public class AssociationList implements Map{
		private LinkedList keysList;
		private LinkedList values;
	public AssociationList(){
		LinkedList keysList = new LinkedList();
		LinkedList values = new LinkedList();
	}
	@Override
	public void put(Object k, Object v) {
		if(keysList.isEmpty()){
			keysList.add(k);
			values.add(v);
		}
		else{
			if(keysList.contains(k)){
				values.add(keysList.indexOf(k), v);
			}
		}
		
	}

	@Override
	public Object remove(Object k) {
		if(keysList.contains(k)){
			values.remove(keysList.indexOf(k));
			keysList.remove(k);
		}
		return k;
	}

	@Override
	public int size() {
		return keysList.size();
	}

	@Override
	public boolean containsKey(Object k) {
		return keysList.contains(k);
	}

	@Override
	public Object get(Object k) {
		if(!keysList.contains(k)){
			throw new NoSuchElementException();
		}
		return values.get(keysList.indexOf(k));
	}

	@Override
	public LinkedList keys() {
		return keysList;
	}

	@Override
	public LinkedList values() {
		return values;
	}

}
